  <resource key="listreport.navigate.i18n" type="com.wabacus.config.resource.TemplateRes">
  		<![CDATA[
  			<table width="100%" border="0" cellspacing="0" cellpadding="0">
  			  <tr>
  				<td align="left"><span class="cls-navigate-info">
  					当前第<wx:navigate type="pageno"/>/<wx:navigate type="pagecount"/>页
